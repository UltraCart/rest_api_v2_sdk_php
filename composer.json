{
    "name": "ultracart/rest_api_v2_sdk_php",
    "description": "UltraCart REST API Version 2",
    "keywords": [
        "openapitools",
        "openapi-generator",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://www.ultracart.com",
    "license": "MIT",
    "authors": [
        {
            "name": "UltraCart",
            "homepage": "https://www.ultracart.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "symfony/polyfill-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "ultracart\\v2\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "ultracart\\v2\\Test\\" : "test/" }
    }
}
